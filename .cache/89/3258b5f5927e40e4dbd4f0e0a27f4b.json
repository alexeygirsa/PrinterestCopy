\n    align-items: center;\r\n    padding: 10px;\r\n    gap: 10px;\r\n}\r\n\r\n.autorImg {\r\n    height: 50px;\r\n    width: 50px;\r\n    border-radius: 50%;\r\n}\r\n\r\n.hidden {\r\n    display: none;\r\n}\r\n\r\n.banned {\r\n    display: none;\r\n}\r\n\r\n.hiddenPinMenu {\r\n    display: none;\r\n}\r\n\r\n.pinTag {\r\n    display: none;\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"07ebf97009b34bf541d19df404982ae9","cacheData":{"env":{}}}